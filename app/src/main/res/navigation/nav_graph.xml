<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/login_page"
    app:startDestination="@id/blankFragment2">

    <fragment
        android:id="@+id/login_page2"
        android:name="com.example.geekmediaapp.login_page"
        android:label="fragment_login_page"
        tools:layout="@layout/fragment_login_page" >
        <action
            android:id="@+id/action_login_page2_to_book_list"
            app:destination="@id/book_list" />
        <action
            android:id="@+id/action_login_page2_to_admin_choice"
            app:destination="@id/admin_choice" />
        <action
            android:id="@+id/action_login_page2_to_register_page2"
            app:destination="@id/register_page2" />
    </fragment>
    <fragment
        android:id="@+id/register_page2"
        android:name="com.example.geekmediaapp.register_page"
        android:label="fragment_register_page"
        tools:layout="@layout/fragment_register_page" >
        <action
            android:id="@+id/action_register_page2_to_login_page2"
            app:destination="@id/login_page2" />
    </fragment>
    <fragment
        android:id="@+id/admin_choice"
        android:name="com.example.geekmediaapp.admin_choice"
        android:label="fragment_admin_choice"
        tools:layout="@layout/fragment_admin_choice" >
        <action
            android:id="@+id/action_admin_choice_to_upload_page"
            app:destination="@id/upload_page" />
        <action
            android:id="@+id/action_admin_choice_to_book_list"
            app:destination="@id/book_list" />
        <action
            android:id="@+id/action_admin_choice_to_login_page2"
            app:destination="@id/login_page2" />
    </fragment>
    <fragment
        android:id="@+id/book_list"
        android:name="com.example.geekmediaapp.book_list"
        android:label="fragment_book_list"
        tools:layout="@layout/fragment_book_list" >
        <action
            android:id="@+id/action_book_list_to_books_page"
            app:destination="@id/books_page" />
    </fragment>
    <fragment
        android:id="@+id/upload_page"
        android:name="com.example.geekmediaapp.upload_page"
        android:label="fragment_upload_page"
        tools:layout="@layout/fragment_upload_page" >
        <action
            android:id="@+id/action_upload_page_to_book_list"
            app:destination="@id/book_list" />
        <action
            android:id="@+id/action_upload_page_to_admin_choice"
            app:destination="@id/admin_choice" />
    </fragment>
    <fragment
        android:id="@+id/books_page"
        android:name="com.example.geekmediaapp.books_page"
        android:label="fragment_books_page"
        tools:layout="@layout/fragment_books_page" />
    <fragment
        android:id="@+id/blankFragment2"
        android:name="com.example.geekmediaapp.BlankFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_blank" >
        <action
            android:id="@+id/action_blankFragment2_to_login_page2"
            app:destination="@id/login_page2" />
    </fragment>
</navigation>